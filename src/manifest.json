{
  "manifest_version": 2,
  "name": "github-notifier",
  "version": "0.1.0",
  "description": "",
  "permissions": [
    "storage",
    "chrome://favicon/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon19.png",
      "38": "images/icon38.png"
    },
    "default_popup": "popup.html"
  },
  "options_page": "options.html"
}
